{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "~5.1",
		"barryvdh/laravel-debugbar": "~2",
		"pingpong/modules": "~2.1",
		"pingpong/generators": "2.1.*@dev",
		"lucadegasperi/oauth2-server-laravel": "5.0.*",
		"zordius/lightncandy": "0.22",
		"proai/laravel-handlebars": "1.0.1",
		"guzzlehttp/guzzle": "~6.0",
		"anthonymartin/geo-location": "^1.0",
		"torann/geoip": "0.2.*@dev",
		"riari/laravel-forum": "~3.0",
		"riari/laravel-forum-frontend": "~1.0",
    "cmgmyr/messenger": "~2.0",
    "mariuzzo/laravel-js-localization": "1.4.*",
		"doctrine/dbal": "^2.5"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
		"laracasts/behat-laravel-extension": "@dev",
		"behat/behat": "~3.0@dev",
	  	"behat/mink": "~1.6@dev",
	  	"behat/mink-extension": "~2.0@dev",
	  	"behat/mink-selenium2-driver": "*",
		"fzaninotto/faker": "*",
		"facebook/webdriver": "dev-master"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"Caravel\\": "app/",
			"Modules\\": "modules/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
