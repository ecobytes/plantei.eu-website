{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.0.*",
		"barryvdh/laravel-debugbar": "~2.0",
		"pingpong/modules": "2.0.*@dev",
		"pingpong/generators": "2.0.*@dev",
		"lucadegasperi/oauth2-server-laravel": "4.0.x@dev",
		"markusjwetzel/laravel-handlebars": "1.0.*@dev",
		"zordius/lightncandy": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
		"laracasts/behat-laravel-extension": "@dev",
		"behat/behat": "~3.0",
		"behat/mink": "~1.6",
		"behat/mink-extension": "~2.0",
		"behat/mink-selenium2-driver": "*",
		"fzaninotto/faker": "*",
		"facebook/webdriver": "dev-master"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"Caravel\\": "app/",
			"Modules\\": "modules/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
