<h2>{{messages.messages}}</h2>
  <table class="table" id="messages">
    <tbody>
      {{#each messenger}}
      <tr data-message_id="{{id}}">
        <td>{{thread.subject}}</td>
        <td>{{user.name}}</td>
        <td>{{updated_at}}</td>
      </tr>
      {{else}}
      <tr>
        <h4>No content</h4>
      </tr>
      {{/each}}
    </tbody>
  </table>
