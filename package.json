{
  "name": "static-site",
  "private": true,
  "dependencies": {
    "async": ">=0.9.0",
    "boom": ">=2.7.0",
    "crumb": ">=4.0.3",
    "debug": ">=2.1.3",
    "deep-equal": ">=1.0.0",
    "good": ">=5.1.2",
    "good-console": ">=4.1.0",
    "handlebars": ">=3.0.1",
    "joi": ">=6.1.0",
    "lodash": ">=3.6.0",
    "nano": ">=6.1.2",
    "object-versions": ">=1.0.0"
  },
  "scripts": {
    "test": "node_modules/.bin/lab -t 100",
    "dev": "npm run build-static && npm run build-static-onchange & npm run copy & npm run serve",
    "build-static": "node build.js",
    "build-static-onchange": "node_modules/.bin/onchange 'templates/**/*' 'src/**/*' -- npm run build-static",
    "copy": "mkdir -p public && node_modules/.bin/onchange 'build/**/*' -- cp -a build/* public",
    "serve": "node_modules/.bin/livereloadx -s -p 9191 ./public",
    "lint": "node utils/html5-lint.js",
    "clean": "rm -rf node_modules src/vendor",
    "postinstall": "./node_modules/.bin/bower install"
  },
  "devDependencies": {
    "code": ">=1.4.0",
    "colors": ">=1.0.3",
    "html5-lint": ">=0.2.5",
    "lab": ">=5.5.1",
    "livereloadx": ">=0.3.7",
    "markdownlint": ">=0.0.3",
    "metalsmith": ">=1.5.0",
    "metalsmith-collections": ">=0.7.0",
    "metalsmith-headings-identifier": ">=0.0.3",
    "metalsmith-less": ">=2.0.0",
    "metalsmith-markdown": ">=0.2.1",
    "metalsmith-permalinks": ">=0.4.0",
    "metalsmith-templates": ">=0.7.0",
    "minimatch": ">=2.0.4",
    "mock-couch": ">=0.1.2",
    "onchange": ">=1.0.0",
    "recursive-readdir": ">=1.2.1"
  }
}
